<view class="word-book"><view class="top-bar"><view data-event-opts="{{[['tap',[['toToday',['$event']]]]]}}" class="{{['top-bar-item '+(activeType===1?'active':'')]}}" bindtap="__e">今日生词</view><view data-event-opts="{{[['tap',[['toAll',['$event']]]]]}}" class="{{['top-bar-item '+(activeType===1?'':'active')]}}" bindtap="__e">所有生词</view></view><view class="word-book-list"><block wx:for="{{wordList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><list-card vue-id="{{'60d0d1a5-1-'+index}}" word="{{item.wordContent}}" mean="{{item.meaning[0]}}" data-word="{{item}}" data-event-opts="{{[['tap',[['onWordClick',['$event',['o',['word',item]]]]]]]}}" bindtap="__e" bind:__l="__l"></list-card></block><view class="diction-list"><block wx:for="{{dictionaryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="diction-list-item"><text>{{item}}</text></view></block></view></view></view>